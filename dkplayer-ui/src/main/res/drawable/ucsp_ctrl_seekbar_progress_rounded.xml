<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/ucsp_ctrl_seekbar_background_color" />
            <corners android:radius="@dimen/ucsp_ctrl_seekbar_height" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <solid android:color="@color/ucsp_ctrl_seekbar_secondary_progress_color" />
                <corners android:radius="@dimen/ucsp_ctrl_seekbar_height" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="@color/ucsp_ctrl_seekbar_progress_color" />
                <corners android:radius="@dimen/ucsp_ctrl_seekbar_height" />
            </shape>
        </clip>
    </item>
</layer-list>
